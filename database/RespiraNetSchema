--
-- TOC entry 224 (class 1255 OID 16474)
-- Name: update_node_last_seen(); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION public.update_node_last_seen() RETURNS pg_trigger
    LANGUAGE plpgsql
    AS $$BEGIN
  UPDATE nodes
  SET last_seen = COALESCE(NEW.ts, now())
  WHERE id = NEW.node_id;

  RETURN NEW;
END;
$$;


ALTER FUNCTION public.update_node_last_seen() OWNER TO postgres;

